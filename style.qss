*{
    font-family:"HarmonyOS Sans SC";
}
/**********页签项**********/
/*页签项默认状态*/
QTabBar::tab {
		
    height: 25px;
    color: rgb(79, 79, 79);
    background: transparent;
    font: 14px;
    margin-right: 10px;
    margin-left: 10px;
}
QTabBar::tab:hover {
    font: 15px;
    border-bottom: 2px solid rgb(155, 155, 155);
    color: black;
    height: 25px;
    margin-right: 10px;
    margin-left: 10px;
}
QTabBar::tab:selected {
    font: 15px solid;
    color: black;
    border-bottom: 2px solid rgb(194, 12, 12) ;
}
QTabWidget#tabWidget::pane {
    border: 1px solid rgb(220, 220, 220);
    background: rgb(250, 250, 250);
    margin-top: 12px;
    color: black;
    border-radius: 10px;
}

/**********按钮**********/
QPushButton{
    border-radius: 8px;
    border: none;
    height: 30px;
    font: 12px;
}
QPushButton:enabled {
    background-color: rgb(0, 126, 57);
    color: white;
}
QPushButton:!enabled {
    background-color: rgb(0, 56, 0);
    color: rgb(200, 200, 200);
}
QPushButton:enabled:hover{
    background: rgb(0, 106, 37);
}
QPushButton:enabled:pressed{
    background: rgb(204, 204, 204);
    font: bold;
}

QPushButton[Gray = "true"] {
    border-radius: 8px;
    border: 1px solid rgb(200, 200, 200);
    color: black;
}
QPushButton[Gray = "true"]:enabled {
    background: rgb(235, 235, 235);
    color: black;
}
QPushButton[Gray = "true"]:!enabled {
    background-color: rgb(92, 92, 92);
    color: rgb(200, 200, 200);
}
QPushButton[Gray = "true"]:enabled:hover {
    background: rgb(220, 220, 220);
}
QPushButton[Gray = "true"]:enabled:pressed {
    background: rgb(210, 210, 210);
    font: bold;
}

QPushButton[Orange = "true"] {
    border-radius: 8px;
    color: white;
}
QPushButton[Orange = "true"]:enabled {
    background: rgb(204, 88, 21);
}
QPushButton[Orange = "true"]:!enabled {
    background-color: rgb(78, 34, 9);
    color: rgb(200, 200, 200);
}
QPushButton[Orange = "true"]:enabled:hover {
    background: rgb(165, 72, 18);
}
QPushButton[Orange = "true"]:enabled:pressed {
    background: rgb(204, 204, 204);
    font: bold;
}
QPushButton[Blue = "true"] {
    border-radius: 8px;
    color: white;
}
QPushButton[Blue = "true"]:enabled {
    background: rgb(31, 91, 182);
}
QPushButton[Blue = "true"]:!enabled {
    background-color: rgb(0, 31, 82);
    color: rgb(200, 200, 200);
}
QPushButton[Blue = "true"]:enabled:hover {
    background: rgb(21, 81, 172);
}
QPushButton[Blue = "true"]:enabled:pressed {
    background: rgb(204, 204, 204);
    font: bold;
}

/**********复选框**********/
QCheckBox{
    
    font: 12px;
}
QCheckBox:enabled:checked{
    color: rgb(0, 106, 37);
}
QCheckBox:enabled:!checked{
    color: rgb(70, 71, 73);
}
QCheckBox:enabled:hover{
    color: rgb(0, 136, 67);
}
QCheckBox:!enabled{
    color: rgb(150, 150, 150);
}
QCheckBox::indicator:!checked{
    height: 30px;
    width: 30px;
    image: url(:/Textures/checkbox_unchecked.png);
}
QCheckBox::indicator:checked{
    height: 30px;
    width: 30px;
    image: url(:/Textures/checkbox_checked.png);
}
QCheckBox::indicator:!enabled:checked{
    height: 30px;
    width: 30px;
    image: url(:/Textures/checkbox_checked_unenabled.png);
}
QCheckBox[Orange = "true"]:enabled:checked{
    color: rgb(112, 45, 6);
}
QCheckBox[Orange = "true"]:enabled:!checked{
    color: rgb(70, 71, 73);
}
QCheckBox[Orange = "true"]:enabled:hover{
    color: rgb(132, 65, 26);
}
QCheckBox[Orange = "true"]:!enabled{
    color: rgb(150, 150, 150);
}
QCheckBox[Orange = "true"]::indicator:!checked{
    height: 30px;
    width: 30px;
    image: url(:/Textures/checkbox_unchecked.png);
}
QCheckBox[Orange = "true"]::indicator:checked{
    height: 30px;
    width: 30px;
    image: url(:/Textures/checkbox_checked_orange.png);
}
QCheckBox[Orange = "true"]::indicator:!enabled:checked{
    height: 30px;
    width: 30px;
    image: url(:/Textures/checkbox_checked_unenabled.png);
}
QCheckBox[Blue = "true"]:enabled:checked{
    color: rgb(21, 61, 152);
}
QCheckBox[Blue = "true"]:enabled:!checked{
    color: rgb(70, 71, 73);
}
QCheckBox[Blue = "true"]:enabled:hover{
    color: rgb(21, 61, 152);
}
QCheckBox[Blue = "true"]:!enabled{
    color: rgb(150, 150, 150);
}
QCheckBox[Blue = "true"]::indicator:!checked{
    height: 30px;
    width: 30px;
    image: url(:/Textures/checkbox_unchecked.png);
}
QCheckBox[Blue = "true"]::indicator:checked{
    height: 30px;
    width: 30px;
    image: url(:/Textures/checkbox_checked_blue.png);
}
QCheckBox[Blue = "true"]::indicator:!enabled:checked{
    height: 30px;
    width: 30px;
    image: url(:/Textures/checkbox_checked_unenabled.png);
}
