/**********进度条**********/
QProgressBar{
    text-align: center;
    color: black;
    border: 1px solid rgb(200, 200, 200);
     border-radius: 10px;
    
    font: 12px;
    height: 25px;
    background: rgb(250, 250, 250);
}
QProgressBar::chunk {
    border-radius: 10px;
    background: rgb(30, 166, 97);
}
QProgressBar:!enabled{
    border: none;
    text-align: center;
    color: white;
    border: 1px solid rgb(200, 200, 200);
     border-radius: 10px;
    
    font: 12px;
    height: 25px;
    background: rgb(170, 170, 170);
}
QProgressBar::chunk:!enabled {
    border-radius: 10px;
    background: rgb(94, 94, 94);
}
