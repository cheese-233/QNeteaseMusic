/***********拖动条***********/
QSlider::groove:horizontal{ 
      height: 10px;  
      border:0px solid rgb(220, 220, 220);
      border-radius:5px;    
      background: rgb(220, 220, 220);
      margin-right: 10px;
      margin-left: 10px;
} 
QSlider::handle:horizontal{ 
    image:url(:/Textures/SliderHandler.png);
    margin: -10px;
    height: 20px;
    width: 20px;
    
} 
QSlider::handle:horizontal:hover{ 
    height: 22px;
    width: 22px;
    
} 
QSlider[Orange = "true"]::handle:horizontal:hover{ 
    height: 22px;
    width: 22px;
    
} 
QSlider[Orange = "true"]::sub-page:horizontal{
    border: 0px solid rgb(220, 220, 220);
     border-radius:5px;
    
    font: 12px;
    background: rgb(194, 12, 12);

}
QSlider[Orange = "true"]::handle:horizontal{ 
    margin: -10px;
    height: 20px;
    width: 20px;
    image:url(:/Textures/SliderHandler_orange.png);
} 
QSlider[Blue = "true"]::handle:horizontal:hover{ 
    height: 22px;
    width: 22px;
    
} 
QSlider[Blue = "true"]::sub-page:horizontal{
    border: 0px solid rgb(220, 220, 220);
     border-radius:5px;
    
    font: 12px;
    background: rgb(71, 131, 222);

}
QSlider[Blue = "true"]::handle:horizontal{ 
    margin: -10px;
    height: 20px;
    width: 20px;
    image:url(:/Textures/SliderHandler_blue.png);
} 
QSlider::sub-page:horizontal{
    border: 0px solid rgb(200, 200, 200);
     border-radius:5px;
    margin-left: 10px;
    
    font: 12px;
    background: rgb(50, 186, 117);

}
QSlider::groove:horizontal:!enabled{ 
      height: 10px;  
      border:0px solid rgb(220, 220, 220);
      border-radius:5px; 
      background: rgb(220, 220, 220);
} 
QSlider::handle:horizontal:!enabled{ 
    margin: -10px;
    height: 20px;
    width: 20px;
    image:url(:/Textures/SliderHandler_Grey.png);
} 
QSlider::sub-page:horizontal:!enabled{
    border: 0px solid rgb(200, 200, 200);
     border-radius:5px;
    
    font: 12px;
    background: rgb(94, 94, 94);

}
